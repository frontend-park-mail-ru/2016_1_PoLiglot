<fest:template xmlns:fest="http://fest.mail.ru" context_name="json">
	<div class="main ">
		<div class="main-menu ">
			<div class="users_header row">
				<div class="col-md-2 user_frame">
					<p class="segoe_print_frame">Вы:</p>
					<h3 class="segoe_print_frame user"></h3>
					<p class="segoe_print_frame ">Очки за игру: <span class="segoe_print_frame myScore"></span></p>
					<p class="segoe_print_frame userRecord">Рекорд: </p>
				</div>
				<div class="col-md-5 div_timer">
					<center>
						<div id="timer"  class="timer"></div>
					</center>
				</div>
				<div class="col-md-2 versus_frame">
					<p class="segoe_print_frame">Соперник:</p>
					<h3 class="segoe_print_frame enemy"></h3>
					<p class="segoe_print_frame ">Очки за игру: <span class="segoe_print_frame enemyScore"></span></p>
					<p class="segoe_print_frame enemyRecord">Рекорд: </p>
				</div>
			</div>
			<div class="shuffle_main_row">
				<center>
				<div class="shuffle_string">

				</div>
				</center>
			</div>
			<div class="question_field row">
				
			</div>
			<div class="row">
				<center>
				<button class=" elem_align  row send">Ответить</button>
				</center>
	  		</div>
		</div>
	</div>
</fest:template>
